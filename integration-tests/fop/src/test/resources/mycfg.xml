<fop version="1.0">

    <use-cache>false</use-cache>
    <renderers>
        <renderer mime="application/pdf">
            <disable-srgb-colorspace>true</disable-srgb-colorspace>
            <fonts>
                <font
                        kerning="yes"
                        embed-url="FreeMono.ttf">
                    <font-triplet name="FreeMono" style="normal" weight="normal" />
                </font>
            </fonts>
        </renderer>
    </renderers>

</fop>
